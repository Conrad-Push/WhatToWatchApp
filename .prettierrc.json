{
  "html": {
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 120,
    "singleQuote": true,
    "semi": true,
    "wrapAttributes": "force-aligned"
  },
  "css": {
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 80,
    "singleQuote": true,
    "semi": true
  },
  "json": {
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 120,
    "singleQuote": true,
    "semi": false
  },
  "javascript": {
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 80,
    "singleQuote": true,
    "semi": true
  },
  "javascriptreact": {
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 80,
    "singleQuote": true,
    "semi": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false
  }
}
